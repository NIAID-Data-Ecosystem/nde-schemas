        "definitions": {
          "citation": {
            "description": "A citation object for a resource which is cited by the dataset (ie- is a derivative of the dataset) , related to the dataset, or from which the dataset was based on (ie- is derived from).",
            "@type": "Thing",
            "type": "object",
            "properties": {
              "author": {
                "description": "Author(s) of the referenced resource",
                "oneOf": [{
                    "$ref": "#/definitions/person"
                  },
                  {
                    "$ref": "#/definitions/organization"
                  }
                ]
              },
              "abstract": {
                "description": "Abstract of the referenced resource",
                "type": "string"
              },
              "datePublished": {
                "description": "Date the referenced resource was published",
                "format": "date",
                "type": "string"
              },
              "description": {
                "description": "Description of the referenced resource",
                "type": "string"
              },
              "name": {
                "description": "Name or title of the citation",
                "type": "string"
              },
              "identifier": {
                "description": "An identifier associated with the citation",
                "type": "string"
              },
              "pmid": {
                "description": "A PubMed identifier of the referenced publication, if available",
                "abstract": "PubMed identifier",
                "type": "string"
              },
              "doi": {
                "description": "A digital object identifier (DOI) of the referenced resource, if available",
                "abstract": "digital object identifier",
